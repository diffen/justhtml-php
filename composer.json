{
  "name": "diffen/justhtml",
  "description": "Pure-PHP HTML5 parser (JustHTML port) with html5lib compliance",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.4"
  },
  "autoload": {
    "psr-4": {
      "JustHTML\\": "src/JustHTML/"
    },
    "classmap": [
      "src/JustHTML/"
    ]
  },
  "bin": [
    "bin/justhtml"
  ],
  "keywords": [
    "html5",
    "parser",
    "dom",
    "selector",
    "justhtml"
  ],
  "require-dev": {
    "masterminds/html5": "^2",
    "voku/simple_html_dom": "^4.8",
    "symfony/dom-crawler": "^5.4",
    "symfony/css-selector": "^5.4"
  }
}
